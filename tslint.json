{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "no-console": false,
      "quotemark": [true, "single", "jsx-double", "avoid-escape", "avoid-template"],
      "indent": [true, "spaces", 2],
      "semicolon": [true, "never"]
    },
    "rulesDirectory": [],
    "linterOptions": {
      "exclude": [
        "src/migration/*.ts"
      ]
    }
}
